{
    "name": " Aurorum-Studio /flarum-better-iframe",
    "description": "This project is an extension of flarum. This extension allows users to add iframe into their posts,with full all the attributes iframe have, and with every attribute editable. It's beased on zerosonesfun/BBCode - iframe, but it has more attributes, and is more suitable for websites than that extension.",
    "keywords": [
         "iframe"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Aurorum-Studio/flarum-better-iframe/issues",
        "source": "https://github.com/Aurorum-Studio/flarum-better-iframe",
        "forum": "https://discuss.flarum.org",
        "forum 2(inbuilding)": "https://dev.aurorum.co"
    },
    "homepage": "https://dl.aurorum.co",
    "funding": [
        {
            "type": "website",
            "url": "https://dl.aurorum.co/funding"
        }
    ],
    "require": {
        "flarum/core": "^1.0.0"
    },
    "require-dev": {
        "flarum/testing": "^1.0.0"
    },
    "authors": [
        {
            "name": "Aurorum",
            "email": "xavierpatrick@163.com",
            "role": "Creator"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aurorum-Studio\\Iframe\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Iframe",
            "category": "feature",
            "icon": {
                "name": "fas fa-browser",
                "backgroundColor": "#ddd",
                "color": "#000"
            }
        }
    }
}